        <Key android:popupKeyboard="@xml/popup_ja" android:popupCharacters="わをんー " android:keyLabel="わ"/>
        <Key android:keyLabel="."/>
        <Key android:codes="@integer/key_return" android:keyEdgeFlags="right" android:isModifier="true" android:iconPreview="@drawable/sym_keyboard_return" android:keyIcon="@drawable/sym_keyboard_return"/>
    </Row>
